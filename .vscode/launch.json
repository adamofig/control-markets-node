{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Nest Framework",
        "program": "${workspaceFolder}/node_modules/@nestjs/cli/bin/nest.js",
        "args": ["start", "--debug", "--watch"],
        "autoAttachChildProcesses": true,
        "restart": true,
        "sourceMaps": true,
        "console": "integratedTerminal",
        "envFile": "${workspaceFolder}/.env"
      }
    ]
}